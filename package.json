{
  "name": "touchgrass",
  "version": "0.1.111",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "bun run --cwd packages/cli dev",
    "build": "bun run --cwd packages/cli build",
    "test": "bun run --cwd packages/cli test",
    "typecheck": "bun run --cwd packages/cli typecheck",
    "web:dev": "bun run --cwd packages/web dev",
    "web:build": "bun run --cwd packages/web build",
    "web:deploy": "bun run --cwd packages/web deploy"
  }
}
